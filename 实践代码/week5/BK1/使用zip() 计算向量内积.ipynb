   "source": [
    "# 计算向量内积\n",
    "\n",
    "# 定义向量a和b\n",
    "\n",
    "a = [1, 2, 3, 4, 5]\n",
    "b = [6, 7, 8, 9, 0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "396e9cf6-a6b8-4d25-9ab4-815bcacbf1e1",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 初始化内积为0\n",
    "dot_product = 0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "f3063c0c-c792-4ba7-b9d4-75c2f4b4c6bf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "向量内积为： 80\n"
     ]
    }
   ],
   "source": [
    "# 使用for循环计算内积\n",
    "for a_i, b_i in zip(a, b):\n",
    "    dot_product += (a_i*b_i)\n",
    "\n",
    "# 打印内积\n",
    "print('向量内积为：', dot_product)"
   ]
  },
