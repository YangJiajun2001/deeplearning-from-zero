   "source": [
    "# 使用列表生成式，获得1 ~ 10之间所有偶数列表\n",
    "even_numbers = [num for num in range(1, 11)\n",
    "               if num % 2 == 0]\n",
    "\n",
    "print(even_numbers)"
   ]
