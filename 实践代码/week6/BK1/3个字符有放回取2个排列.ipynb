   "source": [
    "import itertools\n",
    "\n",
    "string = \"abc\"\n",
    "# 定义元素列表\n",
    "elements = list(string)\n",
    "# 指定重复次数\n",
    "repeat = 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "id": "8af71c1c-df4c-47a6-836d-d6e01fc451f1",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 生成有放回排列\n",
    "permutations = itertools.product(elements, repeat = repeat)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 43,
   "id": "763005a7-2638-4965-9f12-83cfe2f82dbc",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "aa\n",
      "ab\n",
      "ac\n",
      "ba\n",
      "bb\n",
      "bc\n",
      "ca\n",
      "cb\n",
      "cc\n"
     ]
    }
   ],
   "source": [
    "# 遍历并打印所有排列\n",
    "for permutation_idx in permutations:\n",
    "    print(''.join(permutation_idx))"
   ]
  },
