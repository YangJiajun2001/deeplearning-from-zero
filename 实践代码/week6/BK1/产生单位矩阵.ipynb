   "source": [
    "# 自定义函数产生单位矩阵\n",
    "def identity_matrix(size):\n",
    "    matrix = []\n",
    "    for i in range(size):\n",
    "        row = [0]*size\n",
    "        row[i] = 1\n",
    "        matrix.append(row)\n",
    "    return matrix"
   ]
